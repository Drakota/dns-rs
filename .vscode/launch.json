{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug unit tests in library 'parser'",
            "cargo": {
                "args": [
                    "test",
                    "--no-run",
                    "--lib",
                    "--package=parser"
                ],
                "filter": {
                    "name": "parser",
                    "kind": "lib"
                }
            },
            "args": [],
            "cwd": "${workspaceFolder}"
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug integration test 'parser'",
            "cargo": {
                "args": [
                    "test",
                    "--no-run",
                    "--test=parser",
                    "--package=parser"
                ],
                "filter": {
                    "name": "parser",
                    "kind": "test"
                }
            },
            "args": [],
            "cwd": "${workspaceFolder}"
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug executable 'proxy'",
            "cargo": {
                "args": [
                    "build",
                    "--bin=proxy",
                    "--package=proxy"
                ],
                "filter": {
                    "name": "proxy",
                    "kind": "bin"
                }
            },
            "args": [
                "--pa",
                "1.1.1.1",
                "-v"
            ],
            "cwd": "${workspaceFolder}"
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug unit tests in executable 'proxy'",
            "cargo": {
                "args": [
                    "test",
                    "--no-run",
                    "--bin=proxy",
                    "--package=proxy"
                ],
                "filter": {
                    "name": "proxy",
                    "kind": "bin"
                }
            },
            "args": [],
            "cwd": "${workspaceFolder}"
        }
    ]
}